{
  "name": "etdlab",
  "version": "1.0.0",
  "description": "Desktop App For Expert-Tech Dental Lab",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . expert-Tech-dental-lab-app --overwrite --no-asar --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Expert-Tech Dental Lab\"",
    "package-linux": "electron-packager . expert-Tech-dental-lab-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Asim",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "custom-electron-titlebar": "^3.0.10",
    "mysql": "^2.17.1"
  },
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-packager": "^14.0.1"
  }
}
